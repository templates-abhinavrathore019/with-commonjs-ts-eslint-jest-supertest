{
    "rest-client.environmentVariables": {
        "$shared": {
            "prefix": "api",
            "v1": "v/1",
            "v2": "v/2"
        },
        "localhost": {
            "host": "http://localhost:5000",
            "token": "Bearer ",
        },
        "development": {
            "host": "https://dev-backend-user.spydra.app",
            "token": "Bearer ",
        },
        "production": {
            "host": "https://controlplane.spydra.ltd",
            "token": "Bearer ",
        }
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript",
        "typescript"
    ],
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
}